{
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#eee9e9",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0e0e0e",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#111e2f",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#faf9f6",
					"name": "Secondary",
					"slug": "secondary"
				}
			]
		},
		"layout": {
			"contentSize": "1000x",
			"wideSize": "1400px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/poppins-v20-latin-regular.woff2"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/poppins-v20-latin-700.woff2"
							]
						}
					],
					"fontFamily": "\"Poppins\", serif",
					"name": "Poppins",
					"slug": "poppins"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "15"
				},
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--secondary)"
				}
			}
		},
		"typography": {
			"fontFamily": "Poppins",
			"fontWeight": "Regular"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2
}